{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Bun",
  "extends": "./base.json",
  "compilerOptions": {
    "types": ["bun-types"],
    "lib": ["esnext"],
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "bundler",
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "moduleDetection": "force",
    "jsx": "react-jsx"
  }
}
